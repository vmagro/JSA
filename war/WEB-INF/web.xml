<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>JSA</servlet-name>
		<servlet-class>org.jsa.socal.mobile.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSA</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Conventions</servlet-name>
		<servlet-class>org.jsa.socal.mobile.ConventionsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Conventions</servlet-name>
		<url-pattern>/conventions</url-pattern>
	</servlet-mapping>
</web-app>
